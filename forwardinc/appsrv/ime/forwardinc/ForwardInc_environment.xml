<?xml version="1.0" encoding="UTF-8"?>

<ImsEnvironment 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="imsconfig://schema/ImsEnvironment.xsd"
	name="@IMEName"
    description="@IMEDescription"
    imstemplatefile="@RoleDefinitionFile"
    envsettingsfile="@EnvSettingsFile"
    directory="@IMDirName"
    agent="@agent"
    alias="@IMEAlias"
    publicalias="@publicalias"
    publicuser="@publicuser"
    basedir="@baseurl"
    jobtimeout="@jobtimeout"
    corporateuser="@corporateuser" >
    <!--  ******************** System Administrator Information ********************  -->
    <!--SystemAdmin contains a list of roles to assign to the user -->
    <!--SystemAdmin consists of the following attributes: -->
    <!--1. name - DN of the user to configure as a system admin -->
    
    <!--User(s) that should be a system administrator-->

    <SystemAdmin name="@SystemMangerDN">
		<RoleAssignment name="System Manager" />
    </SystemAdmin>
</ImsEnvironment>
	
